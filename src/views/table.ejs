<div class="container-fluid px-4">
    <h1 class="mt-4">Usuarios</h1>
    <ol class="breadcrumb mb-4">
        <li class="breadcrumb-item"><a href="index.html">Dashboard</a></li>
        <li class="breadcrumb-item active">Usuarios</li>
    </ol>
    <div class="card mb-4">
        <div class="card-header">
            <i class="fas fa-table me-1"></i>
            DataTable Users
        </div>
        <div class="card-body">
            <table id="datatablesSimple">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Cellphone</th>
                        <th>ID</th>
                        <th>Password Last Change</th>
                        <th>Created</th>
                        <th>Modified</th>
                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Cellphone</th>
                        <th>ID</th>
                        <th>Password Last Change</th>
                        <th>Created</th>
                        <th>Modified</th>
                    </tr>
                </tfoot>
                <tbody>
                    <% for (let user of users) { %>
                        <tr>
                            <td><%= user.name %></td>
                            <td><%= user.email %></td>
                            <td><%= user.cellphone %></td>
                            <td><%= user.identityCard %></td>
                            <td><%= user.lastChangePwd %></td>
                            <td><%= user.created %></td>
                            <td><%= user.modified %></td>
                        </tr>
                    <% } %> 
                </tbody>
            </table>
        </div>
    </div>
</div>